			{ CALL HJW_TEST..FSP_EX002_U02(#{itemClass},#{type}, #{plantCd}, #{giDt}, #{movType}, #{soNo}, #{soSeq}, #{itemCd},#{giQty}, #{slCd}, #{empNo}) }
        ]]>
    </select>


     <select id="fspEx002U03" parameterType="hashmap"  resultType="hashmap">
        <![CDATA[
			{ CALL HJW_TEST..FSP_EX002_U03(#{plantCd}, #{slCd}, #{trnsSlCd}, #{itemCd}, #{fmBarcode}, #{soType}, #{saItemFlag}, #{empNo}) }
        ]]>
    </select>

      <select id="fspEx002U04" parameterType="hashmap"  resultType="hashmap">
        <![CDATA[
			{ CALL HJW_TEST..FSP_EX002_U04(#{soNo}, #{soSeq}, #{dnNo}, #{empNo}) }
        ]]>
    </select>




</mapper>

